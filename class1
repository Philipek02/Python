#zad 1
x = input()
operand = input()
y = input()
x = int(x)
y = int(y)
def plus(a, b):
    return a + b
def minus(a, b):
    return a - b
def mult(a, b):
    return a * b
def div(a, b):
    return a / b

if operand == "+":
    print(plus(x,y))
elif operand == "-":
    print(minus(x,y))
elif operand == "*":
    print(mult(x,y))
elif operand == "/":
    print(div(x,y))
    
